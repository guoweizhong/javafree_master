<%@ page contentType="text/html; charset=UTF-8" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib uri="http://www.javafree.com/menutag" prefix="jfmenu" %><%@ page import="com.javafree.bases.common.Constants" %><c:set var="ctx" value="${pageContext.request.contextPath}"/><div id="sidebar" class="sidebar responsive"><script type="text/javascript">try{ace.settings.check("sidebar","fixed")}catch(e){};</script><div class="sidebar-shortcuts" id="sidebar-shortcuts"><div class="sidebar-shortcuts-large" id="sidebar-shortcuts-large"><button class="btn btn-danger" onClick="javafree.CenterPanel.submitUrl('${pageContext.request.contextPath}/home')"><i class="ace-icon fa fa-home home-icon"></i></button><button class="btn btn-info" onClick="javafree.CenterPanel.popupWindowIFrame('${ctx}/userportal/doGetChangePasswordForm','修改登录密码',600,350);"><i class="ace-icon fa fa-pencil"></i></button><button class="btn btn-success" onClick="javafree.CenterPanel.iframe('${pageContext.request.contextPath}/userportal/usercenter')"><i class="ace-icon fa fa-camera"></i></button><button class="btn btn-warning" onClick="settings();"><i class="ace-icon fa fa-cog"></i></button></div><div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini"><span class="btn btn btn-danger"></span><span class="btn btn-info"></span><span class="btn btn-success"></span><span class="btn btn-warning"></span></div></div><% String menuName="mainmenu"; menuName=request.getSession().getAttribute(Constants.SUBMENUNAME_KEY)!=null?(String)request.getSession().getAttribute(Constants.SUBMENUNAME_KEY):"mainmenu"; //左菜单所用数据名称 request.getSession().setAttribute(Constants.SUBMENUNAME_KEY,menuName); %><jfmenu:displayerName name="BootstrapTreeMenu" permissions="AuthorityDefaultAdapter"><jfmenu:menu name="<%=menuName%>"></jfmenu:menu></jfmenu:displayerName><div class="sidebar-toggle sidebar-collapse" id="sidebar-collapse"><i class="ace-icon fa fa-angle-double-left" data-icon1="ace-icon fa fa-angle-double-left" data-icon2="ace-icon fa fa-angle-double-right"></i></div><script type="text/javascript">try{ace.settings.check("sidebar","collapsed");if(!("javafree" in window)){window.javafree={}}javafree.currentsubmenu="<%=menuName%>"}catch(e){};</script></div>