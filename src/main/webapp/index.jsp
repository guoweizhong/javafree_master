<%@ page contentType="text/html; charset=UTF-8" %><%@ page import="org.apache.shiro.SecurityUtils" %><html><head><title>登录跳转</title><% boolean isLogin=SecurityUtils.getSubject().isAuthenticated(); if (isLogin) { %><meta http-equiv="refresh" content="0; url=${pageContext.request.contextPath}/home"/><% }else{ %><meta http-equiv="refresh" content="0; url=${pageContext.request.contextPath}/login"/><% } %></head><body><% if(isLogin){ response.sendRedirect("home"); }else { response.sendRedirect("login"); } %></body></html>